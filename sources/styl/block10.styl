.block10
  width 100%
  height auto
  background-color #FFFFFF

  .block10-content
    width 100%
    height auto
    justify-content center
    align-items center
    flex-direction column
    box-sizing border-box
    padding-top 40px
    padding-bottom 40px
    @media (min-width 768px)
      padding-top 70px
      padding-bottom 60px


    .block10-head
      width 100%
      height auto
      margin-bottom 20px
      @media (min-width 768px)
        margin-bottom 30px
      @media (min-width 1280px)
        margin-bottom 40px


      h2
        color #0c0b10
        text-align center

    .logos-block
      width 100%
      height auto
      display flex
      display -webkit-flex
      justify-content center
      align-items center
      flex-direction column
      @media (min-width 768px)
        flex-flow row wrap
        padding 0 50px
        box-sizing border-box
      @media (min-width 1280px)
        align-items flex-end


      .logo
        width 100%
        max-width 227px
        height auto
        margin-bottom 40px
        display flex
        display -webkit-flex
        justify-content center
        align-items center
        @media (min-width 768px)
          width 50%
          margin-bottom 60px
          margin-left 30px
          margin-right 30px
        @media (min-width 1280px)
          margin-bottom 0


        img
          width 100%
          height auto