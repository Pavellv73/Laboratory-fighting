.block11
  width 100%
  height auto
  background-color #FFFFFF

  .block11-content
    width 100%
    height auto
    justify-content center
    align-items center
    flex-direction column
    padding-top 40px
    padding-bottom 40px
    box-sizing border-box
    @media (min-width 768px)
      padding-top 60px
      padding-bottom 60px

    .block11-head
      width 100%
      height auto
      margin-bottom 40px
      @media (min-width 768px)
        margin-bottom 60px

      p
        color #b53926
        font-family "BlissPro-Bold"
        font-size 20px
        font-weight 700
        text-align center
        line-height 25px
        @media (min-width 768px)
          font-size 24px
          line-height 26px
        @media (min-width 1280px)
          font-size 32px
          line-height 36px
          margin-bottom 64px

    .photo-block
      width 100%
      height auto
      display flex
      display -webkit-flex
      justify-content center
      align-items center
      flex-direction column
      @media (min-width 768px)
        flex-flow row wrap
        max-width 875px
      @media (min-width 1280px)
        max-width 905px



      .photo
        width 246px
        height 246px
        overflow hidden
        display flex
        display -webkit-flex
        justify-content center
        align-items center
        margin-bottom 5px
        @media (min-width 768px)
          margin-bottom 11px
          margin-right 5px
          margin-left 5px
          width 209px
          height 209px
        @media (min-width 1280px)
          width 284px
          height 284px
          margin-right 8px
          margin-left 8px
          margin-bottom 16px
        &:nth-of-type(4)
          display none
          @media (min-width 768px)
            display block
        &:nth-of-type(5)
          display none
          @media (min-width 768px)
            display block
        &:nth-of-type(6)
          display none
          @media (min-width 768px)
            display block

        img
          width 100%
          height auto